{
    "env": {
        "browser": true,
        "jquery": true
    },
    "globals": {
        "angular": true,
        "moment": true,
        "_": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": "off",
        "no-unused-vars": "warn"
    }
}